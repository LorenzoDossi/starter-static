{
  "name": "eleventy",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prod": "serve ./dist",
    "dev": "rm -rf ./dist && npm-run-all --parallel dev:assets dev:site",
    "build": "rm -rf ./dist && npm-run-all --parallel build:assets build:site",
    "build:site": "rm -rf dist && ELEVENTY_ENV=production npx @11ty/eleventy",
    "build:assets": "webpack --config ./bundler/webpack.prod.js",
    "dev:site": "rm -rf dist && ELEVENTY_ENV=development npx @11ty/eleventy --serve",
    "dev:assets": "webpack --config ./bundler/webpack.dev.js"
  },
  "keywords": [],
  "author": "Lorenzo Dossi",
  "license": "ISC",
