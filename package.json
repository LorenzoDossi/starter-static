    "@babel/preset-env": "7.15.8",
    "@factorial/eleventy-plugin-twig": "^0.1.0",
    "babel-loader": "8.2.3",
    "clean-webpack-plugin": "4.0.0",
    "css-loader": "6.5.0",
    "file-loader": "6.2.0",
    "lil-gui": "0.11.0",
    "mini-css-extract-plugin": "2.4.3",
    "npm-run-all": "^4.1.5",
    "sass": "1.49.9",
    "sass-loader": "12.6.0",
    "serve": "13.0.2",
